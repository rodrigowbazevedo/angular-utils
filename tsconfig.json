{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@rodrigowba/env": [
        "dist/rodrigowba/env"
      ],
      "@rodrigowba/env/*": [
        "dist/rodrigowba/env/*"
      ],
      "@rodrigowba/ngrx-persist": [
        "dist/rodrigowba/ngrx-persist"
      ],
      "@rodrigowba/ngrx-persist/*": [
        "dist/rodrigowba/ngrx-persist/*"
      ],
      "@rodrigowba/http-common": [
        "dist/rodrigowba/http-common"
      ],
      "@rodrigowba/http-common/*": [
        "dist/rodrigowba/http-common/*"
      ],
      "@rodrigowba/observer-component": [
        "dist/rodrigowba/observer-component"
      ],
      "@rodrigowba/observer-component/*": [
        "dist/rodrigowba/observer-component/*"
      ],
      "@rodrigowba/ngrx-i18n": [
        "dist/rodrigowba/ngrx-i18n"
      ],
      "@rodrigowba/ngrx-i18n/*": [
        "dist/rodrigowba/ngrx-i18n/*"
      ],
      "@rodrigowba/pipes": [
        "dist/rodrigowba/pipes"
      ],
      "@rodrigowba/pipes/*": [
        "dist/rodrigowba/pipes/*"
      ],
      "@rodrigowba/form-utils": [
        "dist/rodrigowba/form-utils"
      ],
      "@rodrigowba/form-utils/*": [
        "dist/rodrigowba/form-utils/*"
      ],
      "@rodrigowba/rxjs-utils": [
        "dist/rodrigowba/rxjs-utils/rodrigowba-rxjs-utils",
        "dist/rodrigowba/rxjs-utils"
      ]
    }
  }
}